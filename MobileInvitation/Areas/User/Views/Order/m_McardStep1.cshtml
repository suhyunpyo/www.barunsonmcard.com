@using MobileInvitation.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "User_Layout_m";
}
<link rel="stylesheet" href="/css/m_order.css?ver=5.1">
<link rel="stylesheet" href="/css/cropper.min.css">
<style>

    textarea.inp {min-height:40px; border:1px solid #ebebeb; box-sizing:border-box;}
    .file_box {
        background-color: #FFFFFF;
        display: flex;
        flex-wrap: wrap;
        flex-direction: column; /*수직 정렬*/
        align-items: center;
        justify-content: center;
    }

    .dvhhmm {
        float: left;
        display: flex;
        align-items: center;
        height: 30px;
        padding: 5px;
    }

    .info_reg_list textarea {
        display: block;
        min-height: 40px;
        max-height: 100px;
        border: 1px solid #ebebeb;
        box-sizing: border-box;
        padding: 10px;
        resize: vertical;
        float: right;
        width: 100% !important;
    }

    .line_del_btn {
        float: right !important;
        width: 20% !important;
        line-height: 40px !important;
        background: #757575 !important;
        color: #fff !important;
        text-align: center !important;
    }

    .del_btn {
        padding-left: 10px !important;
    }

    .fl_wrap .etc_contents {
        float: left;
    }

    .fl_wrap {
        position: relative;
    }

    .number_box {
        position: relative;
    }

    .date_box .ico.date {
        position: absolute;
        right: 15px;
        cursor: auto !important;
    }

    .pop_wrap.sns_pop .layer_pop h3 {
        margin: 0px;
    }

    .pop_wrap.sns_pop .layer_pop p {
        margin: 0px;
        padding: 10px 5px 10px 5px;
    }

    .pop_wrap.main_pop .layer_pop h3 {
        margin: 0px;
    }

    .pop_wrap.main_pop .layer_pop p {
        margin: 0px;
        padding: 10px 5px 10px 5px;
    }

    .money_gift p {
        font-size: 12px !important;
    }

    .order_con.money_account .order_detail {
        display: none;
        text-align: center;
    }

        .order_con.money_account .order_detail p {
            font-size: 12px;
            line-height: 24px;
            color: #757575;
            margin: 38px 0 18px;
        }

    .order_detail p {
        text-align: left;
        margin-top: 10px !important;
    }

    .order_detail h3 {
        text-align: left;
        margin-top: 15px !important;
    }

    .con_title span {
        font-size: 14px !important;
    }
</style>
<!-- sub contents -->
<section class="order mc">
    <div class="sub_con_wrap">
        <!-- 주문 제작단계 -->
        <div class="step_view">
            <ol>
                <li class="on">
                    <span>1</span>
                    <p><strong>정보입력</strong></p>
                </li>
                <li>
                    <span>2</span>
                    <p><strong>영상/이미지 등록</strong></p>
                </li>
                <li>
                    <span>3</span>
                    <p><strong>제작 완료</strong></p>
                </li>
            </ol>
        </div>
        <!-- //주문 제작단계 -->
        <!-- 주문제작 영역 -->
        <div class="order_area area">
            <!-- 주문자 정보 -->
            <div class="order_con">
                <h2>주문자 정보</h2>
                <table class="order_table type01">
                    <caption>주문자 정보</caption>
                    <tr>
                        <th>주문자명</th>
                        <td>
                            <div style="position:relative;">
                                <input type="text" class="inp name_inp wp100" id="Name" placeholder="홍길동" value="@ViewData["Name"]" maxlength="50">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th>휴대폰 번호</th>
                        <td>
                            <div style="position:relative;">
                                <input type="text" class="inp num_inp wp100" placeholder="'-'없이 숫자만 입력하세요." id="CellPhone_Number" onkeyup="chkNumber(this);" inputmode="numeric" value="@ViewData["CellPhone_Number"]" maxlength="20">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th>이메일 주소</th>
                        <td>
                            <div style="position:relative;">
                                <div class="clear">
                                    <input type="text" class="inp type02 wp45 fl" id="Email_Account" value="@ViewData["Email_Account"]" maxlength="30">
                                    <span class="select_wrap type01 fr wp45 self_add">
                                        <select id="selEmail" class="select wp100">
                                            <option>직접 입력</option>
                                            <option>naver.com</option>
                                            <option>nate.com</option>
                                            <option>hotmail.com</option>
                                            <option>hanmail.com</option>
                                            <option>daum.net</option>
                                            <option>gmail.com</option>
                                        </select>
                                    </span>
                                </div>
                                <div class="add_text mt10">
                                    <input type="text" class="inp type02 email_inp wp100" id="Email_Address" value="@ViewData["Email_Address"]" maxlength="20">
                                </div>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <!-- //주문자 정보 -->
            <!-- 기본 정보 -->
            <div class="order_con default_info">
                <h2>기본 정보</h2>
                <table class="order_table type01">
                    <caption>기본 정보</caption>
                    <tr>
                        <th>초대장 URL</th>
                        <td>
                            <div class="url_wrap">
                                <p class="bf_url"><strong>https://www.barunsonmcard.com/m/</strong></p>
                                <div class="url_check wp100">
                                    <input type="text" autocomplete="off" id="Invitation_URL" class="inp wp100" placeholder="영문, 숫자, 특수문자( _ , - )조합 사용(최대 12자)" maxlength="12" value="@ViewData["Invitation_URL"]">
                                    <span class="url_text"></span>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th onclick="layerPopOpen('invitation_title_pop')">초대장 제목 <span class="ico notice">툴팁 아이콘</span></th>
                        <td>
                            <div style="position:relative;">
                                <input type="text" id="Invitation_Title" class="inp wp100" placeholder="준서♡은재 결혼식에 초대합니다." value="@ViewData["Invitation_Title"]" maxlength="200">
                            </div>
                            <!-- 초대장 제목 안내 팝업 -->
                            <div id="greetingsPop" class="pop_wrap tip_pop pop_l invitation_title_pop">
                                <div class="layer_pop">
                                    <h3>초대장 제목</h3>
                                    <div class="img_con">
                                        <img src="/img/order/title_img.jpg" alt="초대장 제목 안내 이미지" />
                                    </div>
                                    <p class="pop_text">SNS공유시 카카오톡,페이스북등의 <strong>메인텍스트</strong>로 사용됩니다.</p>
                                    <div class="btn_wrap type01">
                                        <a href="javascript:;" class="btn type03_1" onclick="popClose()">닫기</a>
                                    </div>
                                </div>
                                <div class="pop_mask"></div>
                            </div>
                            <!-- //초대장 제목 안내 팝업 -->
                        </td>
                    </tr>
                    <tr>
                        <th class="vt">인사말</th>
                        <td>
                            <div class="btn_wrap">
                                <a href="javascript:popGreetingSample();" class="btn type03 sample_btn sp01 wp100">인사말 샘플보기</a>
                            </div>
                            <div class="text_area_wrap" style="position:relative;">
                                <textarea idx="2" id="Greetings" cols="30" rows="10" maxlength="500" class="input_match">@ViewData["Greetings"]</textarea>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th class="tip_box">
                            <span class="tip_btn">
                                방명록 기능
                                <button type="button" class="ico notice tip_btn">툴팁 아이콘</button>
                            </span>
                            <div class="bn_tip m_tip w180">
                                <p>방명록 작성은 실명제가 아니며, <br /> 익명으로도 작성할 수 있습니다. </p>
                                <p>방명록에 등록되는 글은 당사에서 <br /> 관리하거나 책임지지 않습니다. </p>
                            </div>
                        </th>
                        <td>
                            <div class="radio_box">
                                <input type="radio" id="GuestBook_Use_Y" name="GuestBook_Use_YN" class="btn_use" idx="1" checked>
                                <label for="GuestBook_Use_Y">사용함</label>
                                <input type="radio" id="GuestBook_Use_N" name="GuestBook_Use_YN" class="btn_unuse" idx="1">
                                <label for="GuestBook_Use_N" class="ml30">사용안함</label>
                            </div>
                        </td>
                    </tr>
                    <!-- 포토형 청첩장인 경우에만 해당 영역 출력 -->
                    <tr>
                        <th class="vt img_help" onclick="layerPopOpen('share_img_pop')">
                            공유 이미지
                            <span class="ico notice">툴팁 아이콘</span>
                        </th>
                        <td>
                            <!-- 공유 이미지 안내 팝업 -->
                            <div class="pop_wrap pop_l share_img_pop">
